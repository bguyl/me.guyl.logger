{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "Editor/**/*.cs",
            "Runtime/**/*.cs"
          ]
        }
      ],
      "dest": "api",
      "force": true,
      "globalNamespaceId": "Global",
      "filter": "filterConfig.yml",
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": true,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "enumSortOrder": "declaringOrder",
      "allowCompilationErrors": true,
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "globalMetadata":
    {
      "_appTitle": "Guyl's Unity Logger",
      "_appFooter": "GLogger documentation üßë‚Äçüíª",
      "_enableSearch": true,
      "_appLogoPath": "images/logo.png"
    },
    "sitemap":
    {
      "baseUrl": "https://bguyl.github.io/unity-glogger",
      "changefreq": "weekly",
      "fileOptions": {
          "api/*": {
              "changefreq": "daily"
          }
      }
    },
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "index.md",
          "toc.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "xref": [
      "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
    ],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}